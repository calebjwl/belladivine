<div class="section">
  <div class="container">
    <p class="title-text">Gallery</p>
    <div class="grid">
      {{#photo-swipe items=items history=false as |photoswipe|}}
        {{#each items as |item index|}}
          <img class="preview" src={{item.src}} {{action 'open' (hash index=index) target=photoswipe}} />
        {{/each}}
      {{/photo-swipe}}
    </div>
  </div>
</div>
